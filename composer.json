{
  "name"             : "tenantedlaravel/development",
  "description"      : "A flexible, seamless and easy to use multitenancy solution for Laravel",
  "type"             : "library",
  "require"          : {
    "php"              : "^8.1",
    "laravel/framework": "^10.0"
  },
  "require-dev"      : {
    "orchestra/testbench"    : "^8.5",
    "psalm/plugin-laravel"   : "^2.8",
    "larastan/larastan"      : "^2.7",
    "laravel/pint"           : "^1.13",
    "phpstan/phpstan-phpunit": "^1.3"
  },
  "license"          : "MIT",
  "autoload"         : {
    "psr-4": {
      "Tenanted\\": "src/"
    },
    "files": []
  },
  "autoload-dev"     : {
    "psr-4": {
      "Tenanted\\Tests\\": "tests"
    }
  },
  "authors"          : [
    {
      "name" : "Ollie",
      "email": "code@ollie.codes"
    }
  ],
  "minimum-stability": "stable",
  "scripts"          : {
    "post-autoload-dump": "@php vendor/bin/testbench package:discover --ansi",
    "changelog"         : "conventional-changelog",
    "release"           : "conventional-changelog --commit",
    "release:patch"     : "conventional-changelog --patch --commit",
    "release:minor"     : "conventional-changelog --minor --commit",
    "release:major"     : "conventional-changelog --major --commit",
    "psalm"             : "@php vendor/bin/psalm --no-cache --no-reflection-cache",
    "phpstan"           : "@php vendor/bin/phpstan --memory-limit=2G",
    "static"            : [
      "psalm",
      "phpstan"
    ]
  },
  "extra"            : {
    "laravel": {
      "providers": [
        "Tenanted\\Core\\TenantedServiceProvider"
      ]
    }
  }
}
